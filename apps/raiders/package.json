{
  "name": "raiders",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "generate:drizzle": "drizzle-kit generate",
    "preview": "$npm_execpath run build && vite preview",
    "deploy": "$npm_execpath run build && wrangler deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "helper": "npm run start --workspace=scripts --"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.6",
    "drizzle-zod": "^0.8.3",
    "immer": "^10.1.3",
    "wrangler": "^4.40.2",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.13.7",
    "@cloudflare/workers-types": "^4.20251011.0",
    "@tailwindcss/vite": "^4.1.14",
    "@types/node": "^24.5.2",
    "drizzle-kit": "^0.31.5",
    "typescript": "^5.9.2",
    "vite": "^7.1.7"
  }
}
